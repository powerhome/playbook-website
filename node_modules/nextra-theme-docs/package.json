{
  "name": "nextra-theme-docs",
  "version": "2.0.0-beta.15",
  "description": "A Nextra theme for documentation sites.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "files": [
    "dist",
    "style.css"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "callout": [
        "./dist/callout.d.ts"
      ],
      "bleed": [
        "./dist/bleed.d.ts"
      ],
      "tabs": [
        "./dist/tabs.d.ts"
      ],
      "collapse": [
        "./dist/collapse.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./callout": {
      "import": "./dist/callout.js",
      "types": "./dist/callout.d.ts"
    },
    "./bleed": {
      "import": "./dist/bleed.js",
      "types": "./dist/bleed.d.ts"
    },
    "./tabs": {
      "import": "./dist/tabs.js",
      "types": "./dist/tabs.d.ts"
    },
    "./collapse": {
      "import": "./dist/collapse.js",
      "types": "./dist/collapse.d.ts"
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@mdx-js/react": "^2.1.2",
    "@reach/skip-nav": "^0.17.0",
    "classnames": "^2.3.1",
    "flexsearch": "^0.7.21",
    "focus-visible": "^5.2.0",
    "github-slugger": "^1.4.0",
    "intersection-observer": "^0.12.2",
    "match-sorter": "^4.2.0",
    "next-themes": "^0.2.0-beta.2",
    "parse-git-url": "^1.0.1",
    "scroll-into-view-if-needed": "^2.2.29",
    "title": "^3.4.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "*",
    "@tailwindcss/typography": "*",
    "@types/flexsearch": "^0.7.3",
    "@types/react": "*",
    "@types/react-dom": "*",
    "autoprefixer": "*",
    "concurrently": "*",
    "cross-env": "*",
    "cssnano": "*",
    "cssnano-preset-advanced": "^5.3.8",
    "next": ">=12",
    "nextra": "2.0.0-beta.15",
    "postcss": "*",
    "postcss-cli": "*",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "*",
    "tsup": "*",
    "vitest": "*"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "build": "tsup",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "test": "vitest --run",
    "clean": "rimraf ./dist ./style.css",
    "format": "prettier --ignore-path ../../.gitignore --write --list-different ."
  }
}